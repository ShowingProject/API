{
  "name": "Showing-API",
  "version": "0.0.1",
  "description": "Showing Server API",
  "main": "dist",
  "scripts": {
    "dev": "nodemon -w src --exec \"babel-node src --presets es2015,stage-0\"",
    "dev8080": "PORT=8080 npm run dev",
    "build": "babel src -s -D -d dist --presets es2015,stage-0",
    "start": "node dist",
    "prestart": "npm run -s build",
    "test": "eslint src"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module"
    },
    "env": {
      "node": true
    },
    "rules": {
      "no-console": 0,
      "no-unused-vars": 1
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ShowingProject/API"
  },
  "author": "Lee Hanur <kkak1072@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.13.3",
    "cheerio": "^0.22.0",
    "compression": "^1.5.2",
    "cors": "^2.7.1",
    "dotenv": "^4.0.0",
    "express": "^4.13.3",
    "moment": "^2.17.1",
    "mysql": "^2.13.0",
    "replace-string": "^1.1.0",
    "request": "^2.79.0",
    "request-promise": "^4.1.1",
    "resource-router-middleware": "^0.5.1",
    "sequelize": "^3.30.0"
  },
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-core": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "eslint": "^3.1.1",
    "nodemon": "^1.9.2"
  },
  "bugs": {
    "url": "https://github.com/ShowingProject/API/issues"
  },
  "homepage": "https://github.com/ShowingProject/API#readme"
}
